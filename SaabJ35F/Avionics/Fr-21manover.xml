<?xml version="1.0"?>

<PropertyList>

  <path>Fr-21manover.ac</path>

  <animation>
        <type>knob</type>
        <object-name>pwr</object-name>
        <property>/instrumentation/fr21/pwr</property>
        <factor>-30</factor>
        <center>
            <x-m>0.00926</x-m><y-m>-0.03051</y-m><z-m>-0.03657</z-m>
        </center>
        <axis>
            <x>0</x><y>1</y><z>0</z>
        </axis>
        
        <action>
            <binding>
                <command>property-toggle</command>
                <property>/instrumentation/fr21/pwr</property>
            </binding>
            <binding>
              <command>nasal</command>
              <script>SaabJ35F.sound_helper("sound-small");</script>
            </binding>
        </action>
      <hovered>
       <binding>
        <condition>
         <property>instrumentation/tooltip</property>
        </condition>
        <command>set-tooltip</command>
        <tooltip-id>fr21onoff</tooltip-id>
        <label>Radio %s</label>
        <measure-text>Radio off</measure-text>
        <mapping>on-off</mapping>
        <property>/instrumentation/fr21/pwr</property>
       </binding>
     </hovered>
    </animation>

  <animation>
        <type>knob</type>
        <object-name>mode_selector</object-name>
        <property>/instrumentation/fr21/mode</property>
        <factor>-45</factor>
        <center>
            <x-m>0.018994</x-m><y-m>0.03041</y-m><z-m>-0.03328</z-m>
        </center>
        <axis>
            <x>1</x><y>0</y><z>0</z>
        </axis>
        
        <action>
            <binding>
                <command>property-toggle</command>
                <property>/instrumentation/fr21/mode</property>
            </binding>
            <binding>
              <command>nasal</command>
                <script>
                    SaabJ35F.change_mode();
                    SaabJ35F.sound_helper("sound-small");
                </script>
            </binding>
        </action>
      <hovered>
       <binding>
        <condition>
         <property>instrumentation/tooltip</property>
        </condition>
        <command>set-tooltip</command>
        <tooltip-id>fr21onoff</tooltip-id>
        <label>Mode: %s</label>
        <measure-text>Mode: normal</measure-text>
        <property>/instrumentation/fr21/mode</property>
        <mapping>nasal</mapping>
        <script>
          var modes=["Normal", "Reserv"];
          return modes[arg[0]];
        </script>
       </binding>
     </hovered>
    </animation>
    
  <animation>
        <type>knob</type>
        <object-name>vol</object-name>
        <property>/instrumentation/comm[0]/volume</property>
        <factor>300</factor>
        <center>
            <x-m>0.020057</x-m><y-m>-0.047841</y-m><z-m>-0.038199</z-m>
        </center>
        <axis>
            <x>1</x><y>0</y><z>0</z>
        </axis>
        
        <action>
            <binding>
                <command>property-adjust</command>
                <property>/instrumentation/comm[0]/volume</property>
                <factor>0.05</factor>
                <min>0</min>
                <max>1</max>
                <wrap>false</wrap>
            </binding>
            <binding>
                <command>property-adjust</command>
                <property>/instrumentation/comm[1]/volume</property>
                <step>0.05</step>
                <min>0</min>
                <max>1</max>
                <wrap>false</wrap>
            </binding>
        </action>
      <hovered>
       <binding>
        <condition>
         <property>instrumentation/tooltip</property>
        </condition>
        <command>set-tooltip</command>
        <tooltip-id>fr21vol</tooltip-id>
        <label>Volume: %d%%</label>
        <measure-text>Mode: normal</measure-text>
        <property>/instrumentation/comm[0]/volume</property>
        <mapping>percent</mapping>
       </binding>
     </hovered>
    </animation>
    
    
