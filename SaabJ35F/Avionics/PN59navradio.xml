<?xml version="1.0"?>

<PropertyList>

  <path>PN59navradio.ac</path>

 
<!-- Anita1 -->

<animation>
    <name>Anita1Knob</name>
    <type>knob</type>
    <object-name>Anita1</object-name>
    <property>/instrumentation/navradio/anita1</property>
    <factor>-22.5</factor>
    <center>
      <x-m>0.005749</x-m>
      <y-m>-0.02964</y-m>
      <z-m>0.01958</z-m>
    </center>
    <axis>
      <x>1</x>
      <y>0</y>
      <z>0</z>
    </axis>
    <action>
      <binding>
        <command>property-cycle</command>
        <property>/instrumentation/navradio/anita1</property>
        <value type="int">0</value>
        <value type="int">1</value>
        <value type="int">2</value>
        <value type="int">3</value>
        <value type="int">4</value>
        <value type="int">5</value>
        <value type="int">6</value>
        <value type="int">7</value>
        <value type="int">8</value>
        <value type="int">9</value>
        <value type="int">10</value>
        <value type="int">11</value>
        <value type="int">12</value>
        <value type="int">13</value>
        <value type="int">14</value>
        <value type="int">15</value>
        <wrap>false</wrap>
      </binding>
      <binding>
         <command>nasal</command>
         <script>SaabJ35F.set_beacon();</script>
      </binding>
    </action>
       <hovered>
       <binding>
        <condition>
         <property>instrumentation/tooltip</property>
        </condition>
        <command>set-tooltip</command>
        <tooltip-id>anita1</tooltip-id>
        <label>Anita 1: %s</label>
        <measure-text>Anita 1: A</measure-text>
        <property>/instrumentation/navradio/anita1</property>
        <mapping>nasal</mapping>
        <script>
        var t=["A", "B", "C", "D", "E", "F", "G", "H", "I", "J", "K", "L", "M", "O", "P"];
        return t[arg [0]];
        </script>
       </binding>
     </hovered>
 </animation>
 
