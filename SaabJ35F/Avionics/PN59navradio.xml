 <animation>
   <name>SelectorKnob</name>
  <type>knob</type>
   <object-name>Selector</object-name>
   <property>/instrumentation/navradio/mode</property>
   <factor>-30</factor>
   <center>
     <x-m>0.005749</x-m>
     <y-m>-0.02098</y-m>
     <z-m>-0.05628</z-m>
   </center>
   <axis>
     <x>1</x>
     <y>0</y>
     <z>0</z>
   </axis>
   <action>
     <binding>
       <command>property-cycle</command>
       <property>/instrumentation/navradio/mode</property>
       <value type="int">0</value> <!-- FRÅN -->
       <value type="int">1</value> <!-- FÖRV -->
       <value type="int">2</value> <!-- STRIL -->
       <value type="int">3</value> <!-- STRID -->
       <value type="int">4</value> <!-- NAVRIKTN -->
       <value type="int">5</value> <!-- NAV 400 -->
       <value type="int">6</value> <!-- NAV 40 -->
       <value type="int">7</value> <!-- LANDN 40 -->
       <value type="int">8</value> <!-- BARBRO -->
       <wrap>false</wrap>
     </binding>
   </action>
      <hovered>
       <binding>
        <condition>
         <property>instrumentation/tooltip</property>
        </condition>
        <command>set-tooltip</command>
        <tooltip-id>navmode</tooltip-id>
        <label>Nav mode: %s</label>
        <measure-text>Nav mode: A</measure-text>
        <property>/instrumentation/navradio/mode</property>
        <mapping>nasal</mapping>
        <script>
        var t=["FRÅN","FÖRV","STRIL","STRID","NAVRIKTN","NAV 400","NAV 40","LANDN 40","BARBRO"];
        return t[arg [0]];
        </script>
       </binding>
     </hovered>
 </animation>
 

