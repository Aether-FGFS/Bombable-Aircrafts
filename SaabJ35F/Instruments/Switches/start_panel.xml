<?xml version="1.0"?>

<PropertyList>

<path>start_panel.ac</path>

<!-- AB Fuel switch -->
 <animation>
  <type>translate</type>
  <object-name>AB_Fuel</object-name>
  <property>fdm/jsbsim/propulsion/afterburner-pump</property>
  <factor>-0.007</factor>
  <axis>
   <x>1</x>
   <y>0</y>
   <z>0</z>
  </axis>
 </animation>

 <animation>
  <type>pick</type>
  <object-name>AB_Fuel</object-name>
  <action>
   <button>0</button>
   <binding>
    <command>property-cycle</command>
    <property>fdm/jsbsim/propulsion/afterburner-pump</property>
    <value type="double">1</value>
    <value type="double">0</value>
   </binding>
  </action>
        <hovered>
       <binding>
        <condition>
         <property>instrumentation/tooltip</property>
        </condition>
        <command>set-tooltip</command>
        <tooltip-id>abpump</tooltip-id>
        <label>Afterburner pump: %s</label>
        <measure-text>Afterburner pump: off</measure-text>
        <mapping>on-off</mapping>
        <property>fdm/jsbsim/propulsion/afterburner-pump</property>
       </binding>
     </hovered>
 </animation>

<!-- Main Fuel switch and cover-->
 <animation>
  <type>rotate</type>
  <object-name>LP_cover</object-name>
  <property>controls/fuel_cover/position-norm</property>
  <factor>-90</factor>
  <center>
   <x-m>-0.0391</x-m>
   <y-m>0</y-m>
   <z-m>0.0001</z-m>
  </center>
  <axis>
   <x>0</x>
   <y>1</y>
   <z>0</z>
  </axis>
 </animation>

 <animation>
  <type>pick</type>
  <object-name>LP_cover</object-name>
  <action>
   <button>0</button>
   <binding>
    <command>nasal</command>
    <script>SaabJ35F.fuel_cover.toggle();</script>
   </binding>
   <binding>
    <command>nasal</command>
      <script>SaabJ35F.sound_helper("sound-small");</script>
   </binding>
  </action>
 </animation>

 <animation>
  <type>rotate</type>
  <object-name>Main_Fuel</object-name>
  <property>instrumentation/switches/fuel/pos</property>
  <factor>-50</factor>
  <center>
   <x-m>-0.0116</x-m>
   <y-m>0</y-m>
   <z-m>0.0</z-m>
  </center>
  <axis>
   <x>0</x>
   <y>1</y>
   <z>0</z>
  </axis>
 </animation>
 
 <animation>
  <type>pick</type>
  <object-name>Main_Fuel</object-name>
  <condition>
   <greater-than>
    <property>controls/fuel_cover/position-norm</property>
    <value>0.7</value>
   </greater-than>
  </condition>
  <action>
   <button>0</button>
   <binding>
    <command>nasal</command>
    <script>SaabJ35F.lt_switch_toggle();</script>>
   </binding>
   <binding>
    <command>nasal</command>
      <script>SaabJ35F.sound_helper("sound-small");</script>
   </binding>
  </action>
      <hovered>
       <binding>
        <condition>
         <property>instrumentation/tooltip</property>
        </condition>
        <command>set-tooltip</command>
        <tooltip-id>lppump</tooltip-id>
        <label>LP pump: %s</label>
        <measure-text>LP pump: off</measure-text>
        <mapping>on-off</mapping>
        <property>instrumentation/switches/fuel/pos</property>
       </binding>
     </hovered>
 </animation>

<!-- Fuel indicator lamps -->

 <animation>
  <type>material</type>
  <object-name>LT_lamp</object-name>
  <emission>
   <red>1.0</red>
   <green>1.0</green>
   <blue>1.0</blue>
   <factor-prop>instrumentation/fuel/LT_light</factor-prop>
  </emission>
 </animation>
 
 <animation>
  <type>textranslate</type>
  <object-name>LT_lamp</object-name>
  <property>instrumentation/fuel/LT_light</property>
    <factor>-0.09375</factor>
    <axis> 
      <x> 0 </x> 
      <y> 1 </y> 
    </axis>
 </animation>
 
 <animation>
  <type>material</type>
  <object-name>FT_lamp</object-name>
  <emission>
   <red>1.0</red>
   <green>1.0</green>
   <blue>1.0</blue>
   <factor-prop>instrumentation/fuel/FT_light</factor-prop>
  </emission>
 </animation>

 <animation>
  <type>textranslate</type>
  <object-name>FT_lamp</object-name>
  <property>instrumentation/fuel/FT_light</property>
    <factor>-0.09375</factor>
    <axis> 
      <x> 0 </x> 
      <y> 1 </y> 
    </axis>
 </animation>
   
<!-- Battery switch -->
 <animation>
  <type>rotate</type>
  <object-name>Battery_cover</object-name>
  <property>controls/battery_cover/position-norm</property>
  <factor>-90</factor>
  <center>
   <x-m>-0.0165</x-m>
   <y-m>0</y-m>
   <z-m>0.0002</z-m>
  </center>
  <axis>
   <x>0</x>
   <y>1</y>
   <z>0</z>
  </axis>
 </animation>

 <animation>
  <type>pick</type>
  <object-name>Battery_cover</object-name>
  <action>
   <button>0</button>
   <binding>
    <command>nasal</command>
    <script>SaabJ35F.battery_cover.toggle();</script>
   </binding>
  </action>
 </animation>
 
 <animation>
  <type>translate</type>
  <object-name>Battery</object-name>
  <property>controls/electric/battery-switch</property>
  <factor>-0.007</factor>
  <axis>
   <x>1</x>
   <y>0</y>
   <z>0</z>
  </axis>
 </animation>

 <animation>
  <type>pick</type>
  <object-name>Battery</object-name>
  <condition>
   <greater-than>
    <property>controls/battery_cover/position-norm</property>
    <value>0.7</value>
   </greater-than>
  </condition>
  <action>
   <button>0</button>
   <binding>
    <command>property-toggle</command>
    <property>controls/electric/battery-switch</property>
   </binding>
   <binding>
    <command>nasal</command>
    <script>SaabJ35F.battery_cover.toggle();</script>>
   </binding>
  </action>
    <hovered>
       <binding>
        <condition>
         <property>instrumentation/tooltip</property>
        </condition>
        <command>set-tooltip</command>
        <tooltip-id>bsw</tooltip-id>
        <label>Battery: %s</label>
        <measure-text>Battery: off</measure-text>
        <mapping>on-off</mapping>
        <property>controls/electric/battery-switch</property>
       </binding>
     </hovered>
 </animation>

<!-- Generator switch -->

 <animation>
  <type>rotate</type>
  <object-name>Generator</object-name>
  <property>instrumentation/switches/generator/pos</property>
  <factor>-60</factor>
  <offset-deg>30</offset-deg>
  <center>
   <x-m>0.00224</x-m>
   <y-m>0</y-m>
   <z-m>-0.00587</z-m>
  </center>
  <axis>
   <x>0</x>
   <y>1</y>
   <z>0</z>
  </axis>
 </animation>

 <animation>
  <type>pick</type>
  <object-name>Generator</object-name>
  <action>
   <button>0</button>
   <binding>
    <command>nasal</command>
     <script>SaabJ35F.generator_switch(1);</script>
   </binding>
   <binding>
    <command>nasal</command>
      <script>SaabJ35F.sound_helper("sound-small");</script>
   </binding>
    <mod-up>
     <binding>
       <command>nasal</command>
       <script>SaabJ35F.generator_switch(0);</script>     
     </binding>
    </mod-up>
  </action>
      <hovered>
       <binding>
        <condition>
         <property>instrumentation/tooltip</property>
        </condition>
        <command>set-tooltip</command>
        <tooltip-id>gens</tooltip-id>
        <label>Generator: %s</label>
        <measure-text>Generator: off</measure-text>
        <mapping>on-off</mapping>
        <property>controls/electric/engine/generator</property>
       </binding>
     </hovered>
 </animation>

<!-- Start button -->
 
 <animation>
  <type>translate</type>
  <object-name>Start</object-name>
  <property>instrumentation/switches/start/pos</property>
  <factor>-0.0045</factor>
  <axis>
   <x>0</x>
   <y>0</y>
   <z>1</z>
  </axis>
 </animation>
 
 <animation>
  <type>pick</type>
  <object-name>Start</object-name>
  <action>
   <button>0</button>
   <binding>
     <command>nasal</command>
     <script>SaabJ35F.autostart();</script>
   </binding>
   <binding>
     <command>property-assign</command>
     <property>instrumentation/switches/start/pos</property>
     <value>true</value>
   </binding>
    <mod-up>
     <binding>
       <command>property-assign</command>
       <property>instrumentation/switches/start/pos</property>
       <value>false</value>     
     </binding>
    </mod-up>
  </action>
        <hovered>
       <binding>
        <condition>
         <property>instrumentation/tooltip</property>
        </condition>
        <command>set-tooltip</command>
        <tooltip-id>start</tooltip-id>
        <label>Start</label>
       </binding>
     </hovered>
 </animation>

