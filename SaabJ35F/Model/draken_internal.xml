 <!-- Lighting control -->
 
 <animation>
    <name>InstrumentLightKnob</name>
        <type>knob</type>
        <object-name>Inst_light_knb</object-name>
        <property>instrumentation/switches/inst-light-knob/pos</property>
        <factor>-400</factor>
    <center>
      <x-m>-3.751</x-m>
      <y-m>-0.387</y-m>
      <z-m>0.41663</z-m>
    </center>
    <axis>
      <x>0</x>
      <y>0</y>
      <z>1</z>
    </axis>
    <action>
      <binding>
        <command>property-adjust</command>
        <property>instrumentation/switches/inst-light-knob/pos</property>
        <factor>0.05</factor>
        <min>0</min>
        <max>0.5</max>                
        <wrap>false</wrap>
