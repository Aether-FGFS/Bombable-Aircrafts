<!-- Special switches -->

 <animation>
  <type>rotate</type>
  <object-name>stallwset</object-name>
  <property>instrumentation/stall-warn-setting/pos</property>
  <factor>60</factor>
  <center>
   <x-m>-4.079</x-m>
   <y-m>0</y-m>
   <z-m>0.269</z-m>
  </center>
  <axis>
   <x>0</x>
   <y>1</y>
   <z>0</z>
  </axis>
 </animation>

 <animation>
  <type>pick</type>
  <object-name>stallwset</object-name>
  <hovered>
   <binding>
    <condition>
     <property>instrumentation/tooltip</property>
    </condition>
    <command>set-tooltip</command>
    <tooltip-id>pin</tooltip-id>
    <label>Stall W. limit %s</label>
     <property>instrumentation/stall-warn-setting/pos</property>
     <mapping>nasal</mapping>
     <script><![CDATA[
       if (arg[0]) return "Early";
       else return "Normal"]]>
     </script>
   </binding>
  </hovered>
  <action>
   <button>0</button>
   <binding>
    <command>property-cycle</command>
    <property>instrumentation/stall-warn-limit</property>
    <value>15.5</value>
    <value>14.0</value>
   </binding>
   <binding>
    <command>property-toggle</command>
    <property>instrumentation/stall-warn-setting/pos</property>
   </binding>
   <binding>
    <command>nasal</command>
      <script>SaabJ35F.sound_helper("sound-small");</script>
   </binding>
  </action>
 </animation>

 <animation>
  <type>slider</type>
  <object-name>UHFbutton</object-name>
  <property>/instrumentation/fr21/transmit</property>
  <factor>0.002</factor>
  <axis>
      <x>0</x>
      <y>0</y>
      <z>-1</z>
  </axis>
  <action>
     <binding>
         <command>property-assign</command>
         <property>instrumentation/fr21/transmit</property>
         <value>true</value>
    </binding>
    <binding>
      <command>nasal</command>
      <script>
        SaabJ35F.sound_helper("sound-small");        
      </script>
    </binding>
  </action>
  <release>
    <binding>
      <command>property-assign</command>
      <property>instrumentation/fr21/transmit</property>
      <value>false</value>
    </binding>
    <binding>
      <command>nasal</command>
      <script>
      SaabJ35F.stril_message();        
      </script>
  </binding>
  </release>
  <hovered>
      <binding>
          <command>set-tooltip</command>
          <tooltip-id>transmitbtn</tooltip-id>
          <label>Transmit</label>
      </binding>
  </hovered>
 </animation>
 
