<!-- Gear controls -->
 <animation>
  <type>rotate</type>
  <object-name>Gears</object-name>
  <property>/controls/gear/gear-down</property>
  <factor>30</factor>
  <center>
   <x-m>-3.83</x-m>
   <y-m>-0.35</y-m>
   <z-m>0.165</z-m>
  </center>
  <axis>
   <x>0</x>
   <y>1</y>
   <z>0</z>
  </axis>
 </animation>

 <animation>
  <type>pick</type>
   <object-name>Gears</object-name>
   <action>
    <button>0</button>
    <repeatable>false</repeatable>
    <interval-sec>0.2</interval-sec>
    <binding>
      <command>property-toggle</command>
      <property>/controls/gear/gear-down</property>
    </binding>
   </action>
   <hovered>
     <binding>
      <condition>
       <property>instrumentation/tooltip</property>
      </condition>
      <command>set-tooltip</command>
      <tooltip-id>gearleaver</tooltip-id>
      <label>Gears: %s</label>
      <measure-text>Gears: Down</measure-text>
      <property>/controls/gear/gear-down</property>
      <mapping>down-up</mapping>
    </binding>
   </hovered>
 </animation>

 <animation>
  <type>rotate</type>
  <object-name>NoseW_control</object-name>
  <property>/gear/gear[0]/steering-norm</property>
  <factor>-88</factor>
  <center>
   <x-m>-3.92</x-m>
   <y-m>0.324</y-m>
   <z-m>0.026</z-m>
  </center>
  <axis>
   <x>0</x>
   <y>0</y>
   <z>1</z>
  </axis>
 </animation>
 
<!-- Dragchute handle -->

 <animation>
  <type>rotate</type>
  <object-name>Parachute_handle</object-name>
  <property>/controls/dragchute/chute-lever</property>
  <factor>-33</factor>
  <center>
   <x-m>-3.83</x-m>
   <y-m>-0.35</y-m>
   <z-m>0.165</z-m>
  </center>
  <axis>
   <x>0</x>
   <y>1</y>
   <z>0</z>
  </axis>
 </animation>
 
 <animation>
  <type>pick</type>
   <object-name>Parachute_handle</object-name>
   <action>
    <button>0</button>
    <repeatable>false</repeatable>
    <interval-sec>0.2</interval-sec>
    <binding>
	    <command>nasal</command>
			<script>SaabJ35F.chute_handler();</script>
    </binding>
   </action>
   <hovered>
     <binding>
      <condition>
       <property>instrumentation/tooltip</property>
      </condition>
      <command>set-tooltip</command>
      <tooltip-id>paraleaver</tooltip-id>
      <label>Parachute: %s</label>
      <measure-text>Parachute deployed</measure-text>
      <property>/instrumentation/chute_state</property>
      <mapping>nasal</mapping>
      <script>
       var state=["No chute", "Loaded", "Deployed", "Dropped"];
       return state[arg[0]];
      </script>
     </binding>
   </hovered>
 </animation>

<!-- Parking brake handle -->

 <animation>
  <type>translate</type>
  <object-name>P_brake</object-name>
  <property>/controls/gear/brake-parking</property>
  <factor>0.03</factor>
  <axis>
   <x>1</x>
   <y>0</y>
   <z>0</z>
  </axis>
 </animation>

 <animation>
  <type>pick</type>
   <object-name>P_brake</object-name>
   <action>
    <button>0</button>
    <repeatable>false</repeatable>
    <interval-sec>0.2</interval-sec>
    <binding>
      <command>property-toggle</command>
      <property>/controls/gear/brake-parking</property>
    </binding>
    <binding>
    <command>nasal</command>
      <script>SaabJ35F.sound_helper("sound-large");</script>
   </binding>
   </action>
   <hovered>
     <binding>
      <condition>
       <property>instrumentation/tooltip</property>
      </condition>
      <command>set-tooltip</command>
      <tooltip-id>pbrakeleaver</tooltip-id>
      <label>Parking brake: %s</label>
      <measure-text>Parking brake: released</measure-text>
      <property>/controls/gear/brake-parking</property>
      <mapping>nasal</mapping>
      <script>
       var state=["Released", "Set"];
       return state[arg[0]];
      </script>
     </binding>
   </hovered>
 </animation>


