<PropertyList include="draken-bombableinclude.xml">
 
 <path>draken.ac</path>

  <!-- Multiplayer events =========================================-->
  <nasal>
    <load><![CDATA[
      ##############################################################################
      # The on-load Nasal is not executed when this file is loaded as the user
      # aircraft.
      ##############################################################################
      var rplayer = cmdarg();
      # Set up property aliases for animations.
      rplayer.getNode("fdm/jsbsim/fcs/drag-chute-deployed", 1).
       alias(rplayer.getNode("sim/multiplay/generic/int[0]"));
      rplayer.getNode("engines/engine[0]/augmentation", 1).
       alias(rplayer.getNode("sim/multiplay/generic/bool[0]"));
      rplayer.getNode("consumables/droptanks", 1).
       alias(rplayer.getNode("sim/multiplay/generic/bool[1]"));
      rplayer.getNode("controls/canopy/position-norm", 1).
       alias(rplayer.getNode("sim/multiplay/generic/float[0]"));
      rplayer.getNode("fdm/jsbsim/aero/alpha-deg", 1).
       alias(rplayer.getNode("sim/multiplay/generic/float[1]"));
      rplayer.getNode("fdm/jsbsim/aero/beta-deg", 1).
       alias(rplayer.getNode("sim/multiplay/generic/float[2]"));
      io.load_nasal (getprop ("/sim/fg-aircraft") ~ "/SaabJ35F/Nasal/bombable.nas", "SaabJ35F_bombable");
      SaabJ35F_bombable.load(rplayer);
      var livery_update = aircraft.livery_update.new("Aircraft/SaabJ35F/Models/Liveries");]]>
    </load>
    <unload><![CDATA[
      var rplayer = cmdarg();
      print("UNLOAD SaabJ35F ", rplayer.getPath());
      SaabJ35F_bombable.unload(rplayer);
      livery_update.stop();
    ]]></unload>
  </nasal>

