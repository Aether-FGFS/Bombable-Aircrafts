<?xml version="1.0"?>
<PropertyList>

  <path>/Models/Effects/procedural_light.ac</path>

    <animation>
      <type>select</type>
      <object-name>procedural_light</object-name>
      <condition>
          <greater-than>
          <property>/controls/lighting/nav-lights</property>
          <value>0</value>
        </greater-than>
      </condition>
    </animation>

 <animation>
    <type>noshadow</type>
    <object-name>procedural_light</object-name>
 </animation>
 
    <effect>
        <inherits-from>Effects/draken-light-nav-tail</inherits-from>
        <object-name>procedural_light</object-name>
    </effect>

</PropertyList>
