                          0.9000  1.0010
                          1.0000  1.0000
                          1.1000  1.0000
                      </tableData>
                  </table>
        </function>

        <function name="aero/function/CA-1">
           <description>Quotient of inlet velocit and air velocity minus one</description>
           <min>
             <sum>
               <quotient>
                 <product>
                   <property>propulsion/engine/n1</property>
                   <value>4</value> <!-- Approx 400 ft/s at max RPM -->
                 </product>
                 <property>velocities/u-aero-fps</property>
               </quotient>
               <value>-1.0</value>
             </sum>
             <value>9</value> <!-- Avoids unrealistic effects at very low speeds  -->
           </min>
         </function>

