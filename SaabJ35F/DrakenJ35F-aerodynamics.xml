 <aerodynamics>


        <function name="aero/function/kCLge">
            <description>Change_in_lift_due_to_ground_effect</description>
                  <table>
                      <independentVar>aero/h_b-mac-ft</independentVar>
                      <tableData>
                          0.0000  1.2290
                          0.1000  1.2040
                          0.1500  1.1460
                          0.2000  1.1240
                          0.3000  1.1050
                          0.4000  1.0410
                          0.5000  1.0340
                          0.6000  1.0190
                          0.7000  1.0080
