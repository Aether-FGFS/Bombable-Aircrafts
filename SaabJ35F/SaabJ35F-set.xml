    <model>
      <path>Model/draken.xml</path>
      <livery>
          <file type="string">f16green</file>
      </livery>
    </model>

    <multiplay>
      <generic>
        <int n="0" alias="/fdm/jsbsim/fcs/drag-chute-deployed"/>
