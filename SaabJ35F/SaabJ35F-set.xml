<PropertyList>

  <sim>

    <description>Saab J35F Draken</description>
    <long-description>The Saab J35 Draken was a Swedish fighter aircraft manufactured by Saab between 1955 and 1974. The J35 was an effective supersonic Cold War fighter and the first fully supersonic aircraft to be deployed in Western Europe. This is the F-version from the late sixties.</long-description>
    <author>Anders M</author>
    <rating>
      <FDM type="int">3</FDM>
      <systems type="int">3</systems>
      <cockpit type="int">4</cockpit>
      <model type="int">4</model>
    </rating>
    <tags>
      <tag>fighter</tag>
      <tag>saab</tag>
      <tag>jet</tag>
      <tag>supersonic</tag>
      <tag>ifr</tag>
      <tag>coldwar</tag>
      <tag>1960s</tag>
      <tag>1970s</tag>
    </tags>
    <minimum-fg-version>2020.3.0</minimum-fg-version>
    <aircraft-version>0.9.6</aircraft-version> 
    <status>beta</status> 

    <flight-model>jsb</flight-model>
    <aero>DrakenJ35F</aero>

    <previews>
      <preview>
        <type>exterior</type>
        <splash type="bool">true</splash>
        <path>Previews/SaabJ35F-exterior3.jpg</path>
      </preview>
      <preview>
        <type>exterior</type>
        <splash type="bool">true</splash>
        <path>Previews/SaabJ35F-exterior2.jpg</path>
      </preview>
      <preview>
        <type>exterior</type>
        <splash type="bool">true</splash>
        <path>Previews/SaabJ35F-exterior.jpg</path>
      </preview>
      <preview>
        <type>cockpit</type>
        <splash type="bool">true</splash>
        <path>Previews/SaabJ35F-cockpit.jpg</path>
      </preview>
    </previews>
    
    <sound>
      <path>Sounds/sound.xml</path>
    </sound>

    <virtual-cockpit archive="y">true</virtual-cockpit>
    <allow-toggle-cockpit archive="y">false</allow-toggle-cockpit>
    
    <view n="0">
      <internal archive="y">true</internal>
      <config>
        <pitch-offset-deg>-9</pitch-offset-deg>
        <x-offset-m archive="y">0.0</x-offset-m>
        <y-offset-m archive="y">0.90</y-offset-m>
        <z-offset-m archive="y">-3.42</z-offset-m>
      </config>
    </view>

    <view n="102">
      <name>Instrument View</name>
      <type>lookfrom</type>
      <internal archive="y">true</internal>
      <config>
        <from-model type="bool">true</from-model>
        <from-model-idx type="int">0</from-model-idx>
        <pitch-offset-deg>-23</pitch-offset-deg>
        <x-offset-m archive="y">0.0</x-offset-m>
        <y-offset-m archive="y">0.90</y-offset-m>
        <z-offset-m archive="y">-3.42</z-offset-m>
      </config>
    </view>
        
    <view n="101">
      <name>Nav View</name>
      <type>lookfrom</type>
      <internal archive="y">true</internal>
      <config>
        <from-model type="bool">true</from-model>
        <from-model-idx type="int">0</from-model-idx>
        <heading-offset-deg>-60</heading-offset-deg>
        <pitch-offset-deg>-50</pitch-offset-deg>
        <x-offset-m archive="y">0.0</x-offset-m>
        <y-offset-m archive="y">0.75</y-offset-m>
        <z-offset-m archive="y">-3.72</z-offset-m>
      </config>
    </view>
    
    <view n="100">
      <name>Radio View</name>
      <type>lookfrom</type>
      <internal archive="y">true</internal>
      <config>
        <from-model type="bool">true</from-model>
        <from-model-idx type="int">0</from-model-idx>
        <heading-offset-deg>45</heading-offset-deg>
        <pitch-offset-deg>-50</pitch-offset-deg>
        <x-offset-m archive="y">0.0</x-offset-m>
        <y-offset-m archive="y">0.75</y-offset-m>
        <z-offset-m archive="y">-3.82</z-offset-m>
      </config>
