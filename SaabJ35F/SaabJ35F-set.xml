        <bool n="0" alias="/engines/engine[0]/augmentation"/>
        <bool n="1" alias="/consumables/droptanks"/>
        <bool n="2" alias="controls/lighting/landing-lightL"/>
        <bool n="3" alias="controls/lighting/landing-lightR"/>
        <float n="0" alias="/controls/canopy/position-norm"/>
        <float n="1" alias="/fdm/jsbsim/aero/alpha-deg"/>
        <float n="2" alias="/fdm/jsbsim/aero/beta-deg"/>
        <string n="9" type="string"/>
        <int n="10" alias="controls/armament/trigger" />
      </generic>
      <target>
        <dist type="double">0</dist>
        <course type="double">0</course>
        <bearing type="double">0</bearing>
        <alt type="double">0</alt>
      </target>
    </multiplay>

    <chase-distance-m>-21</chase-distance-m>
    
    <systems>
      <path>Systems/systems.xml</path>
      <autopilot>
        <path>Systems/draken_autopilot.xml</path>
      </autopilot>
    </systems>

    <instrumentation>
      <path>Instruments/draken-instruments.xml</path>
    </instrumentation>

    <!--
        <ai>
        <scenario>refueling_demo</scenario>
        </ai>
    -->

    <aircraft-data>
       <path>/instrumentation/comm[0]/frequencies/selected-mhz</path>
       <path>/instrumentation/comm[1]/frequencies/selected-mhz</path>
       <path>/instrumentation/comm[0]/frequencies/standby-mhz</path>
       <path>/instrumentation/comm[1]/frequencies/standby-mhz</path>
    </aircraft-data>

    <help>
      <title>Saab J35F</title>
      <key>
        <name>Ctrl+D</name>
        <desc>Cycle canopy: Locked - Unlocked (opening) - Open - Unlocked (closing). Initially Unlocked (closing).</desc>
      </key>
      <key>
        <name>e</name>
        <desc>Fire ADEN 30mm cannon (100 rounds available, 1700 rounds per minute)</desc>
      </key>
      <key>
        <name>Shift+J</name>
        <desc>Jettison Droptanks in flight</desc>
      </key>
      <key>
        <name>Shift+P</name>
        <desc>Use drag chute handle</desc>
      </key>
      <key>
        <name>Ctrl+A</name>
        <desc>AP: Altitude hold</desc>
      </key>
      <key>
        <name>Ctrl+T</name>
        <desc>AP: Attitude hold</desc>
      </key>
      <key>
        <name>Z</name>
        <desc>Sight: Increased distance</desc>
      </key>
      <key>
        <name>z</name>
        <desc>Sight: Decreased distance</desc>
      </key>      
      <line>Rotate at 270 km/h to 10-12 degrees pitch.  Liftoff at 300 km/h.</line>
      <line>Extract landing gear below 500 km/h.</line>
      <line>Approach at 325 km/h.</line>
      <line>Touchdown at 300 km/h (needs 80-85% RPM).</line>
      <line>Keep nose up until 240 km/h.</line>
      <line>Apply brakes.</line>
      <line>Lower nose at 160 km/h.</line>
    </help>

    <menubar>
      <default>

      <menu n="99">
          <label>Operations</label>
          <enabled type="bool">true</enabled>
          <item>
            <label>Autostart</label>
            <binding>
              <command>nasal</command>
              <script>SaabJ35F.start_systems();</script>
            </binding>
          </item>
          <item>
            <label>Cabin light</label>
	          <binding>
              <command>property-toggle</command>
	            <property>instrumentation/switches/cabin_light/pos</property>
            </binding>
          </item>
        <item>
          <label>Fuel Cutoff</label>
          <binding>
            <condition>
              <property>/sim/input/selected/engine[0]</property>
            </condition>
            <command>property-toggle</command>
            <property>/controls/engines/engine[0]/cutoff</property>
          </binding>
        </item>
        <item>
          <label>Select STRIL target</label>
          <binding>
            <command>nasal</command>
            <script>SaabJ35F.StrilDialog.new(SaabJ35F.verbose);</script>
          </binding>
        </item>
        <item>
          <label>Toggle droptanks (on ground)</label>
          <binding>
            <command>nasal</command>
            <script>SaabJ35F.drophandle(0);</script>
          </binding>
        </item>
          <item>
            <label>Reload parachute (on ground)</label>
            <binding>
              <command>nasal</command>
              <script>SaabJ35F.chute_loader();</script>
           </binding>
        </item>        
        </menu>
        
        <menu n="100">
          <label>J-35 Draken</label>
          <enabled type="bool">true</enabled>
          <item>
            <label>Select Livery</label>
            <binding>
              <command>nasal</command>
              <script>aircraft.livery.dialog.toggle();</script>
            </binding>
          </item>>
          <item>
            <label>Radio frequencies</label>
            <binding>
              <command>nasal</command>
              <script>SaabJ35F.show_radio_dialog();</script>
            </binding>
          </item>
          <item>
            <label>Navigation beacons</label>
            <binding>
              <command>nasal</command>
              <script>SaabJ35F.show_beacon_dialog();</script>
            </binding>
          </item>
          <item>
            <label>Heading dialog</label>
            <binding>
              <command>nasal</command>
              <script>SaabJ35F.open_Compass_Dialog();</script>
            </binding>
          </item>
          <item>
            <label>Toggle Tooltips</label>
            <binding>
             <command>property-toggle</command>
             <property>instrumentation/tooltip</property>
            </binding>
            <binding>
             <command>nasal</command>
             <script>
              var txt=["Tooltip off", "Tooltip on"];
              gui.popupTip(txt[getprop("instrumentation/tooltip")]);
             </script>
            </binding>
          </item>
          <item>
            <label>About J35F</label>
            <binding>
              <command>nasal</command>
              <script>SaabJ35F.show_about_dialog();</script>
            </binding>
          </item>
        </menu>
        
      </default>
    </menubar>
    
    <rendering>
      <redout>
        <enabled type="bool" userarchive="y">true</enabled>
        <parameters>
          <blackout-onset-g type="double">5</blackout-onset-g>
          <blackout-complete-g type="double">9</blackout-complete-g>
          <onset-blackout-sec type="double">300</onset-blackout-sec>
          <fast-blackout-sec type="double">10</fast-blackout-sec>
          <redout-onset-g type="double">-2</redout-onset-g>
          <redout-complete-g type="double">-4</redout-complete-g>
          <onset-redout-sec type="double">45</onset-redout-sec>
          <fast-redout-sec type="double">7.5</fast-redout-sec>
          <recover-fast-sec type="double">7</recover-fast-sec>
          <recover-slow-sec type="double">15</recover-slow-sec>
        </parameters>
      </redout>
      <headshake>
        <enabled type="bool" userarchive="y">true</enabled>
